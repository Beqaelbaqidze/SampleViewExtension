{
  "name": "extensionapi",
  "displayName": "ExtensionApi",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sampleTreeViewContainer",
          "title": "Sample View",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sampleTreeViewContainer": [
        {
          "id": "sampleTreeView",
          "name": "Sample Tree View"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.refreshTreeView",
        "title": "Refresh Sample Tree View"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "node-fetch": "^3.3.2"
  }
}
